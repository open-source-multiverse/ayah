      "sajda": false
    },
    {
      "number": 2,
      "text": {
        "ar": "مَلِكِ ٱلنَّاسِ",
        "en": "The Sovereign of mankind"
      },
      "juz": 30,
      "page": 604,
      "sajda": false
    },
    {
      "number": 3,
      "text": {
        "ar": "إِلَٰهِ ٱلنَّاسِ",
